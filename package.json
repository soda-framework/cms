    "bootstrap-sass": "~3",
    "eonasdan-bootstrap-datetimepicker": "^4.17.37",
    "font-awesome": "^4.5.0",
    "gulp": "^3.9.1",
    "jquery-ui-dist": "^1.12.1",
    "jsoneditor": "^5.5.9",
    "axios": "^0.15.3",
    "cross-env": "^3.2.3",
    "jquery": "^3.1.1",
    "laravel-mix": "0.*",
